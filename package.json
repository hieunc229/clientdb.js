{
  "name": "clientdb.js",
  "version": "0.1.0",
  "description": "A Client-Side Database with Minimalistic Interface for IndexedDB (TypeScript supported)",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/*.test.ts",
    "build": "tsc",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha tests/*.js - - -R spec",
    "build-browser": "tsc -m system --outFile browser/ClientDB.js src/*.ts"
  },
  "author": "Ambi Studio",
  "license": "Apache 2.0",
  "devDependencies": {
    "ts-node": "^8.0.2",
    "typescript": "^3.2.4"
  }
}
